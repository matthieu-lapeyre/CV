@inproceedings{lapeyre:hal-00984312,
    hal_id = {hal-00984312},
    url = {http://hal.inria.fr/hal-00984312},
    title = {{Physical Human-like Interaction with the Acroban and Poppy Humanoid Platforms.}},
    author = {Lapeyre, Matthieu and Rouanet, Pierre and Oudeyer, Pierre-Yves},
    abstract = {{In this paper we will present two humanoid platforms (Acroban and Poppy) developed in our lab to study biped locomotion and physical interaction. We will present how their morphologies (compliance, vertebral column, and soft ma- terials) produce human-like movements and natural responses to external perturbations. We will illustrate this behavior with the example of the socially guided biped locomotion. We will also discuss the very positive emotional reactions aroused when presenting Acroban to the general public. We will hypothesize that this behavior could be explained by the contrast between its crude visual appearance and its smooth and compliant movements which increases the impression of like-likeliness.}},
    language = {Anglais},
    affiliation = {FLOWERS - INRIA Bordeaux - Sud-Ouest},
    booktitle = {{IROS Workshop}},
    address = {Tokyo, Japon},
    audience = {internationale },
    year = {2013},
    month = Nov,
    pdf = {http://hal.inria.fr/hal-00984312/PDF/Rouanet\_Lapeyre\_IROS\_Workshop.pdf},
}

